<h2>Delete/Update Watch</h2>
    
 <body>
    <table>
        <tr>
        <td>Id</td>
        <td>Item Number</td>
        <td> Image</td>
        <td>Description</td>
        <td>price</td>
            </tr>
        
        @foreach(var s in Model)
        {
         <tr>
        <td>@s.Id</td>
        <td>@s.Item_Num</td>
         <td>@s.image</td>
        <td>@s.description</td>
        <td>@s.price</td>
             <td>
                 <a href="~/admin/deleteWatches/@s.Id"> Delete</a>
                 <a href="~/admin/updateWatch/@s.Id"> Update</a>
             </td>
            </tr>
        }
    </table>
     </body>
    
      </html>
